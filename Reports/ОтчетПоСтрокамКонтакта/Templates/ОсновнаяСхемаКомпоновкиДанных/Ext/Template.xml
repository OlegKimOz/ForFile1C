	<dataSet xsi:type="DataSetQuery">
		<name>НаборДанных1</name>
		<field xsi:type="DataSetFieldField">
			<dataPath>Должник</dataPath>
			<field>Должник</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Договор</dataPath>
			<field>Договор</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Реестр</dataPath>
			<field>Реестр</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Комментарий</dataPath>
			<field>Комментарий</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>КонтактноеЛицо</dataPath>
			<field>КонтактноеЛицо</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>РезультатКонтакта</dataPath>
			<field>РезультатКонтакта</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>СтатусЗвонка</dataPath>
			<field>СтатусЗвонка</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Телефон</dataPath>
			<field>Телефон</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ДокументКонтакт</dataPath>
			<field>ДокументКонтакт</field>
		</field>
		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	Контакт.Должник,
	КонтактТелефоны.Телефон,
	КонтактТелефоны.КонтактноеЛицо,
	КонтактТелефоны.РезультатКонтакта,
	КонтактТелефоны.Комментарий,
	КонтактТелефоны.СтатусЗвонка,
	Контакт.Ссылка КАК ДокументКонтакт
ПОМЕСТИТЬ ВТ_Контакты
ИЗ
	Документ.Контакт.Телефоны КАК КонтактТелефоны
		ЛЕВОЕ СОЕДИНЕНИЕ Документ.Контакт КАК Контакт
		ПО КонтактТелефоны.Ссылка = Контакт.Ссылка
{ГДЕ
	(Контакт.Дата &gt;= &amp;НачалПериода
			И Контакт.Дата &lt;= &amp;КонецПериода)}
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Реестры.Ссылка КАК Реестр,
	Реестры.Должник КАК Должник,
	Реестры.Договор
ПОМЕСТИТЬ ВТ_Реестры
ИЗ
	Документ.Реестр.Должники КАК Реестры
ГДЕ
	Реестры.Должник В
			(ВЫБРАТЬ
				ВТ_Контакты.Должник
			ИЗ
				ВТ_Контакты)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_Контакты.Должник,
	ВТ_Реестры.Договор,
	ВТ_Реестры.Реестр,
	ВТ_Контакты.ДокументКонтакт,
	ВТ_Контакты.Телефон,
	ВТ_Контакты.КонтактноеЛицо,
	ВТ_Контакты.РезультатКонтакта,
	ВТ_Контакты.Комментарий,
	ВТ_Контакты.СтатусЗвонка
ИЗ
	ВТ_Контакты КАК ВТ_Контакты
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_Реестры КАК ВТ_Реестры
		ПО ВТ_Контакты.Должник = ВТ_Реестры.Должник</query>
	</dataSet>
