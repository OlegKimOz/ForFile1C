		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	СотрудникиОтделовСрезПоследних.Отдел,
	ПривязкаСотрудникСрезПоследних.Сотрудник,
	ПривязкаСотрудникСрезПоследних.Должник
ПОМЕСТИТЬ ВТ_ОтделСотрудникДолжник
ИЗ
	РегистрСведений.ПривязкаСотрудник.СрезПоследних КАК ПривязкаСотрудникСрезПоследних
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.СотрудникиОтделов.СрезПоследних КАК СотрудникиОтделовСрезПоследних
		ПО ПривязкаСотрудникСрезПоследних.Сотрудник = СотрудникиОтделовСрезПоследних.Сотрудник
ГДЕ
	ПривязкаСотрудникСрезПоследних.Сотрудник.ПометкаУдаления = ЛОЖЬ
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	РеестрДолжники.Ссылка КАК Реестр,
	РеестрДолжники.Ссылка.Банк,
	РеестрДолжники.Должник
ПОМЕСТИТЬ ВТ_Реестр
ИЗ
	Документ.Реестр.Должники КАК РеестрДолжники
ГДЕ
	РеестрДолжники.Ссылка.Проведен = ИСТИНА
	И (&amp;Дата = ДАТАВРЕМЯ(1, 1, 1)
			ИЛИ РеестрДолжники.Ссылка.ДатаПередачи &lt;= &amp;Дата
				И РеестрДолжники.Ссылка.ДатаВозврата &gt; &amp;Дата)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ЕСТЬNULL(ВТ_ОтделСотрудникДолжник.Отдел, "&lt;нет отдела&gt;") КАК Отдел,
	ЕСТЬNULL(ВТ_ОтделСотрудникДолжник.Сотрудник, "&lt;не распределенные&gt;") КАК Сотрудник,
	ВТ_Реестр.Реестр,
	ВТ_Реестр.Банк,
	КОЛИЧЕСТВО(РАЗЛИЧНЫЕ ВТ_Реестр.Должник) КАК КолДолжников
ИЗ
	ВТ_ОтделСотрудникДолжник КАК ВТ_ОтделСотрудникДолжник
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_Реестр КАК ВТ_Реестр
		ПО ВТ_ОтделСотрудникДолжник.Должник = ВТ_Реестр.Должник

СГРУППИРОВАТЬ ПО
	ВТ_ОтделСотрудникДолжник.Отдел,
	ВТ_ОтделСотрудникДолжник.Сотрудник,
	ВТ_Реестр.Реестр,
	ВТ_Реестр.Банк</query>
